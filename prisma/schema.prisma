generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Feedback {
  id        String   @id @default(cuid())
  name      String
  email     String
  remarks   String?
  ratings   Int?
  createdAt DateTime @default(now())
  strMeal   String
}
